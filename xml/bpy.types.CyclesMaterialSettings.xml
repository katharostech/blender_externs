                <desc desctype="attribute" domain="py" noindex="False" objtype="attribute">
                    <desc_signature class="CyclesMaterialSettings" first="False" fullname="CyclesMaterialSettings.displacement_method" ids="bpy.types.CyclesMaterialSettings.displacement_method" module="bpy.types" names="bpy.types.CyclesMaterialSettings.displacement_method"><desc_name xml:space="preserve">displacement_method</desc_name></desc_signature>
                    <desc_content>
                        <paragraph>Method to use for the displacement</paragraph>
                        <bullet_list bullet="*">
                            <list_item>
                                <paragraph><literal>BUMP</literal> Bump Only, Bump mapping to simulate the appearance of displacement.</paragraph>
                            </list_item>
                            <list_item>
                                <paragraph><literal>DISPLACEMENT</literal> Displacement Only, Use true displacement of surface only, requires fine subdivision.</paragraph>
                            </list_item>
                            <list_item>
                                <paragraph><literal>BOTH</literal> Displacement and Bump, Combination of true displacement and bump mapping for finer detail.</paragraph>
                            </list_item>
                        </bullet_list>
                        <field_list>
                            <field>
                                <field_name>Type</field_name>
                                <field_body>
                                    <paragraph>enum in [‘BUMP’, ‘DISPLACEMENT’, ‘BOTH’], default ‘DISPLACEMENT’</paragraph>
                                </field_body>
                            </field>
                        </field_list>
                    </desc_content>
                </desc>
