                    <desc_content>
                        <paragraph>Use transparent shadows for this material if it contains a Transparent BSDF, disabling will render faster but not give accurate shadows</paragraph>
                        <field_list>
                            <field>
                                <field_name>Type</field_name>
                                <field_body>
                                    <paragraph>boolean, default True</paragraph>
                                </field_body>
                            </field>
                        </field_list>
                    </desc_content>
                </desc>
